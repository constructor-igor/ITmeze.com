      <Objects xmlns="">
        <ObjectGroup Name="Itmeze.Blog" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="mongodb://itmeze:Irlandia2003@ds035448.mongolab.com:35448/itmeze_blog" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Itmeze.Blog_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="mongodb://itmeze:Irlandia2003@ds035448.mongolab.com:35448/itmeze_blog" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
